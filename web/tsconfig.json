{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "strict": true,
        "outDir": ".",
        "rootDir": ".",
        "declaration": false,
        "sourceMap": false,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "lib": ["ES2020", "DOM", "DOM.Iterable"]
    },
    "include": ["*.ts"],
    "exclude": ["node_modules", "pkg", "worker.ts"]
}
