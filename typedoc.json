{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "lib/crypto/index.ts",
    "lib/stores/index.ts",
    "lib/hooks/index.ts",
    "lib/transfer/index.ts",
    "lib/network/index.ts",
    "lib/security/index.ts",
    "components/ui/index.ts",
    "components/transfer/index.ts"
  ],
  "entryPointStrategy": "resolve",
  "out": "docs-generated",
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/tests/**",
    "**/.next/**",
    "**/dist/**"
  ],
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeExternals": true,
  "excludeInternal": false,
  "includeVersion": true,
  "sort": ["source-order"],
  "name": "Tallow - P2P File Transfer Documentation",
  "readme": "README.md",
  "theme": "default",
  "darkHighlightTheme": "github-dark",
  "lightHighlightTheme": "github-light",
  "plugin": [],
  "categorizeByGroup": true,
  "categoryOrder": [
    "Hooks",
    "Components",
    "Utilities",
    "Types",
    "*"
  ],
  "searchInComments": true,
  "searchInDocuments": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false,
  "skipErrorChecking": false,
  "gitRevision": "master",
  "gitRemote": "origin",
  "hideGenerator": false,
  "cleanOutputDir": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true,
    "includeFolders": true
  }
}
